{
  "name": "@austin-garrard/funutils",
  "version": "1.9.2",
  "description": "Fun functional utilities for ES6",
  "homepage": "https://github.com/austin-garrard/funutils",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll --testPathIgnorePatterns='perf\\.test'",
    "coverage": "npm run test -- --no-watch-all --coverage",
    "perf": "jest --testPathPattern='perf\\.test' --runInBand --reporters='./perfReporter.js'",
    "lint": "standard --fix"
  },
  "jest": {
    "projects": [
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom",
        "moduleFileExtensions": [
          "js",
          "jsx"
        ],
        "transform": {
          "^.+\\.jsx$": "babel-jest"
        }
      },
      {
        "displayName": "node",
        "testEnvironment": "node",
        "moduleFileExtensions": [
          "js"
        ],
        "transform": {
          "^.+\\.jsx$": "babel-jest"
        }
      }
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "Austin Garrard",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/react": "^10.0.4",
    "babel-jest": "^26.3.0",
    "jest": "^25.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "standard": "^14.3.3",
    "strip-ansi": "^6.0.0"
  }
}
